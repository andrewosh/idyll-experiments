{
  "name": "idyll-fixed-feature",
  "version": "1.0.0",
  "scripts": {
    "start": "idyll index.idl --css styles.css --layout centered --theme github --no-spellcheck",
    "build": "idyll index.idl --layout centered --theme github --css styles.css --build; cp -r {images,fonts} build/;",
    "deploy": "npm run build && gh-pages -d ./build"
  },
  "dependencies": {
    "idyll": "https://github.com/idyll-lang/idyll.git",
    "idyll-default-components": "^1.0.0"
  },
  "devDependencies": {
    "debounce": "^1.0.2",
    "gh-pages": "^0.12.0",
    "regl": "^1.3.0",
    "shallow-equal": "^1.0.0",
    "uglify-js": "^2.7.5",
    "xtend": "^4.0.1"
  }
}
